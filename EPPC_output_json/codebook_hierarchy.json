    {
      "id": "Information-Giving_SDOH",
      "type": "code",
      "description": "Refers to the process of sharing knowledge about the social, economic, and environmental factors that significantly influence an individualâ€™s health and well-being. These include economic stability, education access and quality, health care access and quality, neighborhood and built environment, and social and community context"
    },
    {
      "id": "Economic_stability",
      "type": "subcode",
      "description": "refers to having a steady source of income and access to essential resources needed for a healthy and secure life. This includes being able to afford basic needs such as food, housing and income. It helps individuals feel secure and reduces stress, allowing them to focus on personal growth and well-being."
    },
    {
      "id": "Employment_Status",
      "type": "subsubcode",
      "description": [
        "Refers to an individual's current job situation, such as employed, unemployed, or underemployed"
      ]
    },
    {
      "id": "Socioeconomic_Status_(SES)",
      "type": "subsubcode",
      "description": [
        "A measure of an individual's or household's economic and social position based on income, education, and occupation."
      ]
    },
    {
      "id": "Socioeconomic_Factors",
      "type": "subsubcode",
      "description": [
        "The social and economic conditions that influence an individual's access to resources, opportunities, and overall quality of life."
      ]
    },
    {
      "id": "Housing_Instability",
      "type": "subsubcode",
      "description": [
        "Refers to issues related to the lack of stable or secure housing, including frequent moves or living in temporary accommodations."
      ]
    },
    {
      "id": "Income",
      "type": "subsubcode",
      "description": [
        "The amount of money an individual earns through work or other sources, often determining their purchasing power and economic security."
      ]
    },
    {
      "id": "Financial_Insecurity",
      "type": "subsubcode",
      "description": [
        "The uncertainty or worry about one's ability to meet financial obligations, such as paying bills or securing basic needs."
      ]
    },
    {
      "id": "Work-related_Challenges",
      "type": "subsubcode",
      "description": [
        "Factors related to a person's job, such as physical demands, workplace stress, or unhealthy working conditions, that negatively impact health and well-being."
      ]
    },
    {
      "id": "Housing_Insecurity",
      "type": "subsubcode",
      "description": [
        "The fear or risk of losing one's residence due to financial difficulties, eviction, or other circumstances.",
        "Fear of losing residence"
      ]
    },
    {
      "id": "Education_access_and_quality",
      "type": "subcode",
      "description": ""
    },
    {
      "id": "Health_care_access_and_quality",
      "type": "subcode",
      "description": "refers to the ability to obtain necessary health services and the effectiveness and standard of care patients receive"
    },
    {
      "id": "Substance_Use/Abuse",
      "type": "subsubcode",
      "description": [
        "The excessive or harmful use of substances like alcohol, drugs, or tobacco"
      ]
    },
    {
      "id": "Lifestyle",
      "type": "subsubcode",
